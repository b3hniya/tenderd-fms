{
  "name": "@tenderd-fms/api-client",
  "version": "1.0.0",
  "description": "Auto-generated API client for Tenderd Fleet Management System",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "generate": "openapi --input http://localhost:4000/api-docs-json --output ./src/generated --client axios",
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist src/generated"
  },
  "keywords": [
    "api-client",
    "fleet-management",
    "swagger",
    "openapi"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@tenderd-fms/core-types": "workspace:*",
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "openapi-typescript-codegen": "^0.29.0",
    "typescript": "^5.2.2"
  }
}
